@{
    ViewData["Title"] = "thong tin member";
    var NhtMember = ViewBag.nhtMember as lesson05model.Models.DataModels.NhtMember;
}
<div class="alert alert-danger">
    <h1>Thông tin member</h1>
    <hr />
    <p>Member ID: @NhtMember.NhtMemberId</p>
    <p>Username: @NhtMember.NhtUserName</p>
    <p>Full Name: NhtMember.NhtFullName</p>
    <p>Password: @NhtMember.NhtPassword</p>
    <p>Email: @NhtMember.NhtEmail</p>
</div>
